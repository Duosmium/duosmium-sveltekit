		"@types/async": "3.2.18",
		"async": "3.2.4",
		"dotenv": "^16.0.3",
		"js-yaml": "^4.1.0",
		"mongodb": "^5.0.1",
		"prisma": "^4.11.0",
		"sciolyff": "^0.11.0",
		"strftime": "^0.10.1"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@types/js-yaml": "^4.0.5",
		"@types/strftime": "^0.9.4",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
